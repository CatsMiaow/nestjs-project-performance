{
  "name": "nestjs-prisma-starter",
  "version": "0.0.0",
  "description": "Node.js Nest framework starter with Prisma",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "vitest run",
    "test:e2e": "vitest run --config ./test/vitest.e2e.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.4",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "^10.0.4",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.4",
    "@nestjs/serve-static": "^4.0.0",
    "@prisma/client": "^4.16.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.0",
    "@nestjs/testing": "^10.0.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.67",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^18.16.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-vitest": "^0.2.6",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "supertest": "^6.3.3",
    "typescript": "~5.0.4",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.32.2"
  }
}
