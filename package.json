{
  "name": "nestjs-project-performance",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project for performance",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "lint": "eslint **/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "nest build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "vitest run --exclude **/e2e/**/*.ts",
    "test:e2e": "vitest run --config ./test/vitest.e2e.ts",
    "entity:load": "ts-node --files ./bin/entities",
    "entity:sync": "ts-node --files ./bin/synchronize"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/passport": "^3.0.2",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.1.1",
    "@mikro-orm/core": "^6.4.7",
    "@mikro-orm/mysql": "^6.4.7",
    "@mikro-orm/nestjs": "^6.1.1",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.0.10",
    "@nestjs/serve-static": "^5.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-pino": "^4.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@inquirer/prompts": "^7.3.2",
    "@mikro-orm/cli": "^6.4.7",
    "@mikro-orm/entity-generator": "^6.4.7",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/testing": "^11.0.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.1",
    "@types/node": "^22.13.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@vitest/eslint-plugin": "^1.1.33",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.7",
    "vitest-mock-extended": "^3.0.1"
  }
}
